{
    "name": "wesrc/secretery",
    "description": "Your personal secretery - A secure place for shared infos",
    "license": "MIT",
    "homepage": "http://secretery.github.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.1.2",
        "zendframework/zend-developer-tools": "dev-master",
        "doctrine/doctrine-orm-module": "0.7.0",
        "zf-commons/zfc-user-doctrine-orm": "0.1.1",
        "zf-commons/zfc-admin": "dev-master",
        "gedmo/doctrine-extensions": "2.3.4",
        "bjyoungblood/bjy-authorize": "1.1.1",
        "mwillbanks/zfc-twitter-bootstrap": "dev-master",
        "dflydev/markdown": "1.0.2",
        "sensiolabs/security-checker": "1.1"
    },
    "scripts": {
        "post-install-cmd": [
            "vendor/bin/security-checker security:check"
         ],
        "post-update-cmd": [
            "vendor/bin/security-checker security:check"
        ]
    }

}
