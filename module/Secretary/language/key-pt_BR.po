msgid "Save public key by using:"
msgstr "Salvar chave pública usando:"

#: ../view/secretary/key/index.phtml:23 ../src/Secretary/Form/Key.php:56s
msgid "Create Key"
msgstr "Criar chave"

#: ../view/secretary/key/index.phtml:39 ../src/Secretary/Form/Key.php:56
msgid "Add own Key"
msgstr "Adicionar própria chave"

#: ../view/secretary/key/index.phtml:24
msgid "You already created a private key"
msgstr "Você já criou uma chave privada"

#: ../src/Secretary/Form/Key.php:79
msgid "Passphrase"
msgstr "Frase secreta"

#: ../src/Secretary/Form/Key.php:92
